public class App {
    public static void main(String[] args) {

    }
    
    public class FriendsHandler {
        public String[] fetchFriends(String profileLink) {
            if(profileLink == "MyLink") {
            	return new String[]{"Alice", "Bob", "Charlie"};
            }
            else return new String[]{"Empty list :("};
        }
    }
